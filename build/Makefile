# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Storia_config = debug

else ifeq ($(config),release)
  Storia_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := Storia

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Storia:
ifneq (,$(Storia_config))
	@echo "==== Building Storia ($(Storia_config)) ===="
	@${MAKE} --no-print-directory -C . -f Storia.make config=$(Storia_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Storia.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Storia"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"